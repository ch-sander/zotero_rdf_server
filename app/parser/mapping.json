{
    "@type": [
      "SemanticNote"
    ],
    "Document": {
      "tags": [
        "p"
      ],
      "types": [
        "Doc"
      ]
    },
    "Annotation": {
      "Concept": {
        "tags": [
          "em"
        ],
        "styles": [
          "font-style:italic"
        ],
        "types": [
          "Annotation",
          "Concept"
        ]
      },
      "Entity": {
        "tags": [
          "strong"
        ],
        "types": [
            "Annotation",
            "Entity"
          ]
      },
      "Entity2": {
        "regex": [
          "#\\\\w+#?"
        ],
        "types": [
          "Annotation",
          "Entity"
        ]
      },
      "Reference1": {
        "tags": [
          "u"
        ],
        "types": [
          "Reference"
        ]
      },
      "Reference2": {
        "tags": [
          "span"
        ],
        "class": "citation",
        "types": [
          "Reference Zotero"
        ]
      }
    },
    "Locator": {
      "tags": [
        "code"
      ],
      "types": [
        "Page-Locator"
      ]
    },
    "Structure": {
      "tags": [
        "h1",
        "h2",
        "h3"
      ],
      "types": [
        "Structure"
      ]
    },
    "Quotation": {
      "tags": [
        "blockquote"
      ],
      "types": [
        "Quotation"
      ]
    },
    "IGNORE": {
      "tags": [
        "pre",
        "img"
      ],
      "regex": [
        "#"
      ]
    },
    "@context": {
      "prov": "http://www.w3.org/ns/prov#",
      "owl": "http://www.w3.org/2002/07/owl#",
      "@vocab": "https://semantic-html.org/vocab#",
      "note": {
        "@id": "inNote",
        "@type": "@id"
      },
      "structure": {
        "@id": "inStructure",
        "@type": "@id"
      },
      "locator": {
        "@id": "hasLocator",
        "@type": "@id"
      },
      "sameAs": {
        "@type": "@id"
      },
      "doc": {
        "@id": "inDoc",
        "@type": "@id"
      },
      "level": {
        "@id": "doco:hasLevel",
        "@type": "xsd:int"
      },
      "generatedAtTime": {
        "@id": "prov:generatedAtTime",
        "@type": "xsd:dateTime"
      }
    }
  }